package com.vti.dto;

import lombok.Data;
import lombok.NonNull;

@Data
public class AccountDTO {
	@NonNull
	private int id;
	
	@NonNull
	private String username;
	
	@NonNull
	private String firstName;
	
	@NonNull
	private String lastName;
	
	@NonNull
	private String email;
	
	@NonNull
	private String role;
	
	@NonNull
	private String status;
	
	@NonNull
	private String departmentName;

}
